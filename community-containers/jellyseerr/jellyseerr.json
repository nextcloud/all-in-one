{
    "aio_services_v1": [
        {
            "container_name": "nextcloud-aio-jellyseerr",
            "display_name": "Seerr",
            "documentation": "https://github.com/nextcloud/all-in-one/tree/main/community-containers/jellyseerr",
            "image": "ghcr.io/seerr-team/seerr",
            "image_tag": "latest",
            "internal_port": "5055",
            "restart": "unless-stopped",
            "init": true,
            "ports": [
                {
                    "ip_binding": "%APACHE_IP_BINDING%",
                    "port_number": "5055",
                    "protocol": "tcp"
                }
            ],
            "environment": [
                "PORT=5055",
                "TZ=%TIMEZONE%"
            ],
            "volumes": [
                {
                    "source": "nextcloud_aio_jellyseerr",
                    "destination": "/app/config",
                    "writeable": true
                }
            ],
            "backup_volumes": [
                "nextcloud_aio_jellyseerr"
            ]
        }
    ]
}
